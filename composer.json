    "keywords": ["laravel", "posts", "facebook", "Slack", "notifications", "wall"],
    "authors": [
        {
            "name": "HighSolutions",
            "email": "adam@highsolutions.pl"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "~5.1",
        "facebook/graph-sdk": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "HighSolutions\\Poster\\": "src/"
        }
    },
    "extra": {
        "component": "package",
        "frameworks": ["Laravel 5.4"]
    },
    "minimum-stability": "stable"
}
